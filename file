def return_value
pipeline
{
	agent any 
	
	stages
	{
		stage('python')
		{
			steps
			{
			git 'https://github.com/esmy1990/locust-experiments.git'
			script
				{
			
 				bat("script: python g.py > merge.txt")
				if  (grep -q "1" "$merge.txt") 
				then
         			echo 'build failed'
				currentBuild.result = 'FAILED'
				else
				echo "continue"
         		
fi
  				
  
				}

			}
			}
		}
	}
}
