def return_value
pipeline
{
	agent any 
	
	stages
	{
		stage('python')
		{
			steps
			{
			git 'https://github.com/esmy1990/locust-experiments.git'
			script
				{
			
 				sh "python g.py ; return_value = returnStatus"
 				if ( return_value == true )
				echo "failure"
   				currentBuild.result = 'UNSTABLE'
				
				}
			}
		}
	}
}
