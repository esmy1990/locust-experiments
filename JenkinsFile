pipeline {
agent any
stages {
    stage('checkout')
	{
        steps 
			{
                git 'https://github.com/esmy1990/locust-experiments.git'
                       
            }
	
	}

    stage('docker build')
	{
        steps
			{
			 sh "docker build -t esmy1990/locust ."
               
            }
    }
	stage('docker push')
	{
        steps
		{
			withCredentials([string(credentialsId: 'dockerhubpwd', variable: 'dockerpwd')]) 
			{
		
			sh "docker login -u esmy1990 -p ${dockerpwd}"
			}
		
			sh "docker  push esmy1990/locust:latest"
		}
    }
    stage('plot')
	{
		steps
		{
			plot csvFileName: 'plot-bfb1b1b2-988f-468f-a986-74bd041dc489.csv', csvSeries: [[displayTableFlag: false, exclusionValues: '', file: 'example_stats.csv', inclusionFlag: 'OFF', url: '']], group: 'locust', style: 'line', title: 'Name', yaxis: 'Avg response time'
		} 
	}
}

}
