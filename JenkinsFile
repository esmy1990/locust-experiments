pipeline {
agent any


    stage('docker build')
	{
        steps
			{
			 sh "docker build -t esmy1990/locust ."
               
            }
    }
	stage('docker push')
	{
        steps
		{
			withCredentials([string(credentialsId: 'dockerhubpwd', variable: 'dockerpwd')]) 
			{
		
			sh "docker login -u esmy1990 -p ${dockerpwd}"
			}
		
			sh "docker  push esmy1990/locust:latest"
		}
    }
    
}
}
