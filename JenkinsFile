pipeline {
agent any
stages {
    stage('checkout')
	{
        steps 
			{
                git 'https://github.com/esmy1990/locust-experiments.git'
                       
            }
	
	}

    stage('docker build')
	{
        steps
			{
			 sh "docker build -t esmy1990/locust ."
               
            }
    }
	stage('docker push')
	{
        steps
		{
			withCredentials([string(credentialsId: 'dockerhubpwd', variable: 'dockerpwd')]) 
			{
		
			sh "docker login -u esmy1990 -p ${dockerpwd}"
			}
		
			sh "docker  push esmy1990/locust:latest"
		}
    }
    stage('plot')
	{
		steps
		{
			plot csvFileName: 'plot-a4b9d641-a6f0-4d63-81b6-9ad3d7c5a2b3.csv', csvSeries: [[displayTableFlag: false, exclusionValues: '', file: 'example_stats.csv', inclusionFlag: 'OFF', url: '']], group: '', numBuilds: '2', style: 'line'}
		} 
	}
}
}
